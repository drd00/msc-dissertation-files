:ManagerContactPoint rdf:type vcard:Kind ;
    vcard:fn "Manager contact page" ;
    vcard:hasURL <https://stanfordmlgroup.github.io/competitions/chexpert/> .

:ChexpertDataset :managerContact :ManagerContactPoint .

# Erratum
:ErratumPresenceMeasurement rdf:type dqv:QualityMeasurement ;
    dqv:computedOn :ChexpertDataset ;
    dqv:isMeasurementOf :ErratumPresenceMetric ;
    dqv:value "false"^^xsd:boolean .

# Update details
:UpdatePresenceMeasurement rdf:type dqv:QualityMeasurement ;
    dqv:computedOn :ChexpertDataset ;
    dqv:isMeasurementOf :UpdatePresenceMetric ;
    dqv:value "false"^^xsd:boolean .

# Data retention limits
:DataRetentionPresenceMeasurement rdf:type dqv:QualityMeasurement ;
    dqv:computedOn :ChexpertDataset ;
    dqv:isMeasurementOf :DataRetentionPresenceMetric ;
    dqv:value "true"^^xsd:boolean .

:DataRetentionDescriptionMeasurement rdf:type dqv:QualityMeasurement ;
    dqv:computedOn :ChexpertDataset ;
    dqv:isMeasurementOf :DataRetentionDescriptionMetric ;
    dqv:value """
        The images and reports used to build the dataset are subject to retention policies of medical records in the Stanford Hospital. There are no limits on the retention of the information in the dataset.
    """^^xsd:string .

# Legacy version support
:LegacySupportPresenceMeasurement rdf:type dqv:QualityMeasurement ;
    dqv:computedOn :ChexpertDataset ;
    dqv:isMeasurementOf :LegacySupportPresenceMetric ;
    dqv:value "false"^^xsd:boolean .

# N.B. this question is supposed to be answered, but wasn't answered.
:LegacySupportDescriptionMeasurement rdf:type dqv:QualityMeasurement ;
