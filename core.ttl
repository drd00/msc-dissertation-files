@prefix airo: <http://example.org/airo/> .
@prefix vair: <http://example.org/vair/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

airo:hasComponent rdfs:domain airo:AISystem ;
#    rdfs:range airo:AISystem , airo:Model .    # airo:Model is now defined in AIRO as a subclass of airo:AIComponent, so this is already supported.
    rdfs:range airo:AISystem .

# Describe the purpose of the model.
airo:hasPurpose rdfs:domain airo:Model ;
    rdfs:range airo:Purpose .

# A model has a dataset component (that used for training)
# vair:Dataset rdf:type owl:Class ;   # Assuming that owl is used.
#     rdfs:subClassOf dcat:Dataset .
# UPDATE 25.06.2024; this has now been added to AIRO, with airo:Dataset
# Being a subclass of airo:AIComponent.

# A model has an algorithm component
vair:Algorithm rdf:type owl:Class . # As of 25.06 still not an AIRO class, but from the VAIR vocabulary.

# Model has dataset and algorithm components
airo:hasComponent rdfs:domain airo:Model ;
    rdfs:range vair:Dataset , vair:Algorithm .
