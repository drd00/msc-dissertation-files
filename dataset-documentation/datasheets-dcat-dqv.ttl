@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix airo: <https://w3id.org/airo#> .

# Example namespace for any new types.
@prefix : <https://example.org/ex#> .

# Has purpose
:PurposeDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Purpose of dataset"@en ;
	rdfs:comment "Describes for what purpose was the dataset created."@en .


# Funded by
:FundingPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
    rdfs:label "Funding presence"@en ;
    rdfs:comment "Indicates whether funding was received for a particular dataset"@en .

:FundingDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
    rdfs:label "Funding description"@en ;
    rdfs:comment "Describes dataset funding, if present."@en .


# Comprised of instances
:InstanceTypesDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Instance types"@en ;
	rdfs:comment "Describes the types of instances comprising the dataset"@en .


# Number of instances
:InstanceCountDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Instance count"@en ;
	rdfs:comment "Counts of instances in the dataset"@en .


# Instances subset or all
:SamplingDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Sampling information"@en ;
	rdfs:comment "Indicates if the dataset is complete (i.e., not a sample (of a larger dataset)). In this implementation, 'complete' is interpreted as 'true' (xsd:boolean) and 'sample' is interpreted from 'false' (xsd:boolean)"@en .

:RepresentativenessDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Representativeness"@en ;
	rdfs:comment "Describes how representative the sample is of the larger set"@en .

:SamplingMethodDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Sampling method"@en ;
	rdfs:comment "Describes the method used for sampling"@en .


# Each instance consists of
:InstanceCompositionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Instance composition"@en ;
	rdfs:comment "Describes what each instance in the dataset consists of"@en .

:FeatureDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Feature description"@en ;
	rdfs:comment "Provides a detailed description of the features in each instance"@en .


# Label or target
:LabelPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Label presence"@en ;
	rdfs:comment "Indicates whether labels or targets are associated with instances."@en .

:LabelDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Label description"@en ;
	rdfs:comment "Provides a detailed description of the labels or targets associated with instances"@en .


# Missing information
:MissingInfoPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of missing information"@en ;
	rdfs:comment "Indicates whether any information is missing from individual instances"@en .

:MissingInfoDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of missing information"@en ;
	rdfs:comment "Provides details about what information is missing and why"@en .


# Relationships made explicit
:RelationshipPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of Explicit Relationships"@en ;
	rdfs:comment "Indicates whether relationships between individual instances are made explicit"@en .

:RelationshipDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of Relationships"@en ;
	rdfs:comment "Provides detail about how relationships between instances are made explicit"@en .


# Data splits
:DataSplitPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of Recommended Data Splits"@en ;
	rdfs:comment "Indicates whether recommended data splits are provided"@en .

:DataSplitDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of Data Splits"@en ;
	rdfs:comment "Provides details about recommended data splits and their rationale."@en .


# Errors, noise, redundancies
:DataIssuesPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of data issues"@en ;
	rdfs:comment "Indicates whether there are known errors, sources of noise, or redunancies in the data"@en .

:DataIssuesDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of data issues" ;
	rdfs:comment "Provides details about known errors, sources of noise, and redunancies in the dataset" .


# External resources
:ExternalResourcesDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "External resources"@en ;
	rdfs:comment "Indicates whether the dataset links to or relies on external resources"@en .

:ExternalResourcesDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "External resources description"@en ;
	rdfs:comment "Descriptions of all external resources and any restrictions associated with them, as well as links or other access points, as appropriate, if the dataset links to or relies on external resources" .
:ResourceGuaranteesDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "External resource guarantees"@en ;
	rdfs:comment "Describes guarantees about the existence and consistency of external resources over time"@en .

:ArchivalVersionsDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Archival versions"@en ;
	rdfs:comment "Describes the availability of offical archival versions of the complete dataset"@en .

:ExternalRestrictionsDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "External resource restrictions"@en ;
	rdfs:comment "Describes any restrictions associated with the external resources"@en .


# Confidential data
:ConfidentialDataPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of confidential data"@en ;
	rdfs:comment "Indicates whether the dataset contains data that might be considered confidential"@en .

:ConfidentialDataDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of confidential data"@en ;
	rdfs:comment "Provides details about the nature of confidential data in the dataset"@en .


# Offensive data
:SensitiveContentPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of potentially offensive or anxiety-causing content"@en ;
	rdfs:comment "Indicates whether the dataset contains data that might be offensive, insulting, threatening, or might cause anxiety if viewed directly"@en .

:SensitiveContentDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of potentially offensive or anxiety-causing content"@en ;
	rdfs:comment "Provides details about the nature of potentially offensive or anxiety-causing content"@en .


# Subpopulations
:SubpopulationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of identified subpopulations"@en ;
	rdfs:comment "Indicates whether the dataset identifies subpopulations"@en .

:SubpopulationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of subpopulations"@en ;
	rdfs:comment "Provides details about how subpopulations are identified and their distributions within the dataset"@en .


# Identifiable individuals
:IndividualIdentificationPossibilityDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Possibility of individual identification"@en ;
	rdfs:comment "Indicates whether it is possible to identify individuals from the dataset, either directly or indirectly"@en .

:IndividualIdentificationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of individual identification possibilities"@en ;
	rdfs:comment "Provides details about how individuals might be identified from the dataset"@en .


# Sensitive data
:SensitiveDataPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Presence of sensitive data"@en ;
	rdfs:comment "Indicates the presence of data which might be considered sensitive"@en .

:SensitiveDataDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Description of sensitive data"@en ;
	rdfs:comment "Provides details about how data might be considered sensitive."@en .


# Instance data acquisition
:InstanceAcquisitionMeans rdf:type owl:Class ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates a category to describe how the data associated with instances was acquired"@en ;
	rdfs:label "Instance acquisition means"@en .

:DirectlyObservable rdf:type :InstanceAcquisitionMeans ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates that a dataset instance was acquired from a directly observable source, e.g. raw text or movie ratings"@en ;
	rdfs:label "Directly observable"@en .

:SubjectReported rdf:type :InstanceAcquisitionMeans ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates that a dataset instance was acquired via reports from subjects (e.g., survey responses)"@en ;
	rdfs:label "Subject reported"@en .

:IndirectInferenceDerived rdf:type :InstanceAcquisitionMeans ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates that a dataset instance was acquired via indirect infererence / derivation from other data (e.g., part-of-speech tags, model-based guesses for age or language)"@en ;
	rdfs:label "Indirectly inferred/derived"@en .

:InstanceAcquisitionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:label "Instance acquisition"@en ;
	rdfs:comment "Indicates how the data was associated with each instance."@en .

:InstanceAcquisitionVerificationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether it was verified how the data was associated with each instance."@en ;
	rdfs:label "Instance acquisition verification"@en.

:InstanceAcquisitionVerificationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how the data was associated with each instance"@en ;
	rdfs:label "Instance acquisition verification description"@en .


# Data acquisition mechanisms
:DataAcquisitionMechanismDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates what mechanisms or procedures were used to collect the data."@en ;
	rdfs:label "Data acquisition mechanism"@en .

:DataAcquisitionMechanismValidationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the data acquisition mechanism was verified"@en ;
	rdfs:label "Data acquisition mechanism validation"@en .

:DataAcquisitionMechanismValidationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how the data acquisition mechanism was verified"@en ;
	rdfs:label "Data acquisition mechanism validation description"@en .


# Collection duration
:DatespanDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates the timeframe over which the data was collected"@en ;
	rdfs:label ""@en .

:DatespanMatchesInstanceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the data associated with the instances matches the timeframe over which the data was collected"@en ;
	rdfs:label ""@en .

:InstanceDateDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes the timeframe in which the data associated with the instances was created, if the data associated with the instances does not matches the timeframe over which the data was collected"@en ;
	rdfs:label "Instance data creation timeframe"@en .


# Ethical review
:EthicalReviewPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether an ethical review was conducted"@en ;
	rdfs:label "Ethical review presence"@en .

:EthicalReviewDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes the ethical review processes, including the outcomes, as well as a link or other access point to any supporting documentation, if an ethical review was conducted on the dataset."@en ;
	rdfs:label "Ethical review description"@en .


# Third-party
:ThirdPartyDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether data was collected from individuals directly, or obtained via third parties or other sources (e.g. websites)"@en ;
	rdfs:label "Obtained directly or via third-party"@en .


# Provided notification
:ProvidedNotificationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether individuals in question were notified about the data collection"@en ;
	rdfs:label "Notification provided"@en .

:ProvidedNotificationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how the individuals in question were notified about the data collection, if they were notified"@en ;
	rdfs:label "Notification provided description"@en .


# Consent obtained
:ConsentObtainedPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the individuals in question consented to the collection and use of their data"@en ;
	rdfs:label "Consent obtained"@en .

:ConsentObtainedDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "If consent was obtained, describes how consent was requested and provided, providing a link or other access point to, or otherwise reproducing, the exact language to which the individuals consented."@en ;
	rdfs:label "Consent obtained description"@en .


# Consent revokation mechanism
:ConsentRevokationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether a mechanism to revoke consent in the future or for certain uses was provided to users, if consent was obtained."@en ;
	rdfs:label "Consent revokation"@en .

:ConsentRevokationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes the consent revokation mechanism provided to individuals if present, as well as a link or other access point to the mechanism (if appropriate)"@en ;
	rdfs:label "Consent revokation description"@en .


# Impact analysis
:ImpactAnalysisPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether an analysis of the potential impact of the dataset and its use on data subject (e.g., a data protection impact analysis) has been conducted"@en ;
	rdfs:label "Impact analysis"@en .

:ImpactAnalysisDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes the impact analysis, if present, including the outcomes, as well as a link or other access point to any supporting documentation"@en ;
	rdfs:label "Impact analysis description"@en .


# Preprocessing data
:PreprocessingDataPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether any preprocessing / cleaning / labelling of the data was done (e.g., discretization or bucketing, tokenization, part-of-speech tagging, SIFT feature extraction, removal of instances, processing of missing values)"@en ;
	rdfs:label "Preprocessing / cleaning / labelling"@en .

:PreprocessingDataDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes the preprocessing / cleaning / labelling which was carried out on the dataset."@en ;
	rdfs:label "Describes how data was preprocessed / cleaned / labelled, if this was done"@en .


######################################################
# Raw data saved and software used only required if 
# Preprocessing / cleaning / labelling was carried out
######################################################
# Raw data saved
:RawDataSavedPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether raw data was saved in addition to the preprocessed/cleaned/labelled data (e.g., to support unanticipated future uses)"@en ;
	rdfs:label "Raw data saved"@en .

:RawDataSavedAccessPointDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Provides a link or other access point to the 'raw' data, if raw data was saved"@en ;
	rdfs:label "Raw data link or other access point"@en .


# Software available
:SoftwareAvailablePresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the software that was used to preprocess / clean / label the data is available."@en ;
	rdfs:label "Preprocessing software availability"@en .

:SoftwareAvailableAccessPointDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Provides a link or other access point to the software used."@en ;
	rdfs:label "Preprocessing software access point"@en .


# Tasks used for
:TasksUsedForPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset has been used for any tasks already"@en ;
	rdfs:label "Dataset used for previous tasks"@en .

:TasksUsedForDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes the tasks which the dataset has already been used for, if applicable"@en ;
	rdfs:label "Description of previous tasks"@en .


# Links to papers or systems
:RepositoryLinksPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there is a repository that links to any or all papers or systems that use the dataset"@en ;
	rdfs:label "Links repository"@en .

:RepositoryLinksAccessPointDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Provides a link or other access point to the repository linking any or all papers or systems that use the dataset"@en ;
	rdfs:label "Links repository access point"@en .


# Other possible tasks
:OtherPossibleTasksPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there are (other) tasks which the dataset could be used for"@en ;
	rdfs:label "Other possible tasks"@en .

:OtherPossibleTasksDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes (other) possible tasks, if there are (other) tasks which the dataset could be used for"@en ;
	rdfs:label "Other possible tasks description"@en .


# Data composition, collection / processing impact
:CompositionCollectionProcessingFutureImpactPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there is anything about the composition of the dataset or the way it was collected and preprocessed/cleaned/labelled that might impact future uses"@en ;
	rdfs:label "Composition / collection / processing future impact"@en .

:CompositionCollectionProcessingFutureImpactDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes anything about the composition of the dataset or the way it was collected and preprocessing/cleaned/labelled that migth impact future uses, if applicable"@en ;
	rdfs:label "Description of composition / collectiong / processing future impact"@en .


# Not other possible tasks
:NotToBeUsedForTaskPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there are tasks for which the dataset should not be used"@en ;
	rdfs:label "Tasks not to be used for"@en .

:NotToBeUsedForTaskDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes the tasks for which the dataset should not be used, if applicable"@en ;
	rdfs:label "Tasks not to be used for description"@en .


# Third party distribution
:ThirdPartyDistributionPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset will be distributed to third parties outside of the entity (e.g., company, institution, organisation) on behalf of which the dataset was created"@en ;
	rdfs:label "Third party distribution"@en .

:ThirdPartyDistributionDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how the dataset will be distributed to third parties outside of the entity (e.g., company, institution, organisation) on behalf of which the dataset was created, if applicable"@en ;
	rdfs:label "Third party distribution description"@en .


# Distribution means
:DistributionMeansDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how the dataset will be distributed (e.g., tarball on website, API, GitHub)"@en ;
	rdfs:label "Distribution means"@en .

:hasDOIDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the datashet has a digital object identifier (DOI)"@en ;
	rdfs:label "Has DOI"@en .

:DOIDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Specifies the DOI for the dataset, if applicable"@en ;
	rdfs:label "Dataset DOI"@en .

# Distribution date
:DistributionDateDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates the date that the dataset will be distributed"@en ;
	rdfs:label "Distribution date"@en .


# License
:LicensePresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset will be distributed under a copyright or other intellectual property (IP) license, and/or under applicable terms of use (ToU)"@en ;
	rdfs:label "License"@en .

:LicenseDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of the license and/or ToU, including a link or other access point to, or otherwise reproducing, any relevant licensing terms or ToU, as well as any fees associated with these restrictions"@en ;
	rdfs:label "Description of licesning and/or ToU, including a link or other access point, if applicable"@en .


# Restrictions on instance data
:InstanceDataRestrictionPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether any third parties imposed IP-based or other restrictions on the data associated with the instances"@en ;
	rdfs:label "Third party restrictions on instance data"@en .

:InstanceDataRestrictionDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how third parties impose IP-based or other restrictions on the data associated with the instances, if applicable"@en ;
	rdfs:label "Description of third party restrictions on instance data"@en .


# Export controls / regulatory restrictions
:ExportControlRegulatoryRestrictionPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether any export controls or other regulatory restrictions apply to the dataset or to individual instances"@en ;
	rdfs:label "Export controls / regulatory restrictions"@en .

:ExportControlRegulatoryRestrictionDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes these restrictions, providing a link or other access point to, or otherwise reproducing, any supporting documentation"@en ;
	rdfs:label "Description of export controls / regulatory restrictions, if applicable"@en .

####################################################
# Consider maintainer and manager contact as
# Subproperties of the dcat:contactPoint property.
####################################################
# Dataset maintainer
:maintainer rdf:type owl:ObjectProperty ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates who will be supporting / hosting / maintaining the dataset"@en ;
	rdfs:label "Dataset supporter / host / maintainer"@en ;
	rdfs:subPropertyOf dcat:contactPoint .


# Manager contact means
:managerContact rdf:type owl:ObjectProperty ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates how the owner / curator / manager of the dataset can be contacted (e.g., email address)"@en ;
	rdfs:label "Manager contact"@en ;
	rdfs:subPropertyOf dcat:contactPoint .


# Erratum
:ErratumPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset has an erratum"@en ;
	rdfs:label "has erratum"@en .

:ErratumAccessPointDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Provides a link or other access point to the erratum, if applicable"@en ;
	rdfs:label "Erratum access point"@en .


# Update details
:UpdatePresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset will be updated (e.g., to correct labelling errors, add new instances, delete instances)"@en ;
	rdfs:label "Dataset updates"@en .

:UpdateFrequencyDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how often updates will be communicated to dataset consumers, if applicable"@en ;
	rdfs:label "Dataset updates frequency"@en .

:UpdaterDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates who will be updating the dataset"@en ;
	rdfs:label "Dataset updater"@en .

:UpdateCommunicationDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how updates will be communicated to dataset consumers, if applicable"@en ;
	rdfs:label "Dataset communication means"@en .


# Data retention limits
:DataRetentionPresenceDimension rdf:type dqv:Dimension ;
    terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
    rdfs:comment "If the dataset relates to people, indicates whether there are applicable limits on the retention of the data associated with the instances (e.g., were the individuals in question told that their data would be retained for a fixed period of time and then deleted)"@en ;
	rdfs:label "Data retention"@en .

:DataRetentionDescriptionDimension rdf:type dqv:Dimension ;
    terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
    rdfs:comment "Describes data retention limits and how they will be enforced, if applicable"@en ;
	rdfs:label "Data retention description"@en .


# Legacy support
:LegacySupportPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether older versions of the dataset continue to be supported / hosted / maintained"@en ;
	rdfs:label "Legacy support"@en .

# N.B. The following must be filled whether legacy support is enabled or not
:LegacySupportDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of how legacy versions will or will not be supported / hosted / maintained"@en ;
	rdfs:label "Legacy support description"@en .


# Augmentation mechanism
:AugmentationPossibilityPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there is a mechanism through which other may extend/augment/build on / contribute to the dataset"@en ;
	rdfs:label "Augmentation possibility"@en .

:AugmentationPossibilityDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of how others may extend/augment/build on / contribute to the dataset, if applicable"@en ;
	rdfs:label "Augmentation possibility description"@en .

:AugmentationValidationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether contributions to the dataset will be validated/verified, if applicable"@en ;
	rdfs:label "Augmentation validation"@en .

# N.B. also needs to be filled out if no validation is carried out
:AugmentationValidationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of how contributions to the dataset will be validated/verified, or why it will not be validated/verified, if applicable"@en ;
	rdfs:label "Augmentation validation description"@en .

:AugmentationCommunicationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there is a process for communicating / distributing contributions to dataset consumers"@en ;
	rdfs:label "Augmentation communication"@en .

:AugmentationCommunicationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of the process for communicating / distributing contributions to dataset consumers"@en ;
	rdfs:label "Augmentation communication description"@en .
