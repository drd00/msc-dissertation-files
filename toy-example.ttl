# include namespace declarations

# Example scenario (ABox):
:provider rdf:type airo:AIProvider .
:ex1 a :RacialBiasAssertion ;
    earl:assertedBy :provider ;
    :acceptableThreshold "0.05"^^xsd:float ;
    :recordedValue "0.1"^^xsd:float .
    earl:outcome earl:failed .
:dataset rdf:type vair:Dataset .
:algorithm rdf:type vair:Algorithm .
:mybiasaudit rdf:type :BiasAudit ;
    :hasAssertion :ex1 .

# Bias audit for a dataset
:dataset :hasAudit :mybiasaudit .
:mymodel rdf:type airo:Model ;
    airo:hasComponent :dataset , :algorithm .

:examplesystem rdf:type airo:AISystem ;
    airo:hasComponent :mymodel .
