@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix airo: <https://w3id.org/airo#> .

# Example namespace for any new types.
@prefix : <https://example.org/ex#> .

# Has purpose
:PurposeDimension rdf:type dqv:Dimension ;
	terms:title "Purpose of dataset" ;
	terms:description "Describes the purpose of the dataset" .


# Comprised of instances
:InstanceTypesDimension rdf:type dqv:Dimension ;
	terms:title "Instance types" ;
	terms:description "Describes the types of instances comprising the dataset" .


# Number of instances
:InstanceCountDimension rdf:type dqv:Dimension ;
	terms:title "Instance count" ;
	terms:description "Counts of instances in the dataset" .


# Instances subset or all
:SamplingDimension rdf:type dqv:Dimension ;
	terms:title "Sampling information" ;
	terms:description "Describes whether the dataset is complete or a sample" .

:RepresentativenessDimension rdf:type dqv:Dimension ;
	terms:title "Representativeness" ;
	terms:description "Describes how representative the sample is of the larger set" .

:SamplingMethodDimension rdf:type dqv:Dimension ;
	terms:title "Sampling method" ;
	terms:description "Describes the method used for sampling" .


# Each instance consists of
:InstanceCompositionDimension rdf:type dqv:Dimension ;
	terms:title "Instance composition" ;
	terms:description "Describes what each instance in the dataset consists of" .

:FeatureDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Feature description" ;
	terms:description "Provides a detailed description of the features in each instance" .


# Label or target
:LabelPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Label presence" ;
	terms:description "Indicates whether labels or targets are associated with instances." .

:LabelDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Label description" ;
	terms:description "Provides a detailed description of the labels or targets associated with instances" .


# Missing information
:MissingInfoPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of missing information" ;
	terms:description "Indicates whether any information is missing from individual instances" .

:MissingInfoDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of missing information" ;
	terms:description "Provides details about what information is missing and why" .


# Relationships made explicit
:RelationshipPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of Explicit Relationships" ;
	terms:description "Indicates whether relationships between individual instances are made explicit" .

:RelationshipDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of Relationships" ;
	terms:description "Provides detail about how relationships between instances are made explicit" .


# Data splits
:DataSplitPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of Recommended Data Splits" ;
	terms:description "Indicates whether recommended data splits are provided" .

:DataSplitDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of Data Splits" ;
	terms:description "Provides details about recommended data splits and their rationale." .


# Errors, noise, redundancies
:DataIssuesPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of data issues" ;
	terms:description "Indicates whether there are known errors, sources of noise, or redunancies in the data" .

:DataIssuesDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of data issues" ;
	terms:description "Provides details about known errors, sources of noise, and redunancies in the dataset" .


# External resources
:ExternalResourcesDimension rdf:type dqv:Dimension ;
	terms:title "External resources" ;
	terms:description "Describes whether the dataset links to or relies on external resources" .

:ResourceGuaranteesDimension rdf:type dqv:Dimension ;
	terms:title "External resource guarantees" ;
	terms:description "Describes guarantees about the existence and consistency of external resources over time" .

:ArchivalVersionsDimension rdf:type dqv:Dimension ;
	terms:title "Archival versions" ;
	terms:description "Describes the availability of offical archival versions of the complete dataset" .

:ExternalRestrictionsDimension rdf:type dqv:Dimension ;
	terms:title "External resource restrictions" ;
	terms:description "Describes any restrictions associated with the external resources" .


# Confidential data
:ConfidentialDataPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of confidential data" ;
	terms:description "Indicates whether the dataset contains data that might be considered confidential" .

:ConfidentialDataDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of confidential data" ;
	terms:description "Provides details about the nature of confidential data in the dataset" .


# Offensive data
:SensitiveContentPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of potentially offensive or anxiety-causing content" ;
	terms:description "Indicates whether the dataset contains data that might be offensive, insulting, threatening, or might cause anxiety if viewed directly" .

:SensitiveContentDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of potentially offensive or anxiety-causing content" ;
	terms:description "Provides details about the nature of potentially offensive or anxiety-causing content" .


# Subpopulations
:SubpopulationPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of identified subpopulations" ;
	terms:description "Indicates whether the dataset identifies subpopulations" .

:SubpopulationDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of subpopulations" ;
	terms:description "Provides details about how subpopulations are identified and their distributions within the dataset" .


# Identifiable individuals
:IndividualIdentificationPossibilityDimension rdf:type dqv:Dimension ;
	terms:title "Possibility of individual identification" ;
	terms:description "Indicates whether it is possible to identify individuals from the dataset, either directly or indirectly" .

:IndividualIdentificationDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of individual identification possibilities" ;
	terms:description "Provides details about how individuals might be identified from the dataset" .


# Sensitive data
:SensitiveDataPresenceDimension rdf:type dqv:Dimension ;
	terms:title "Presence of sensitive data" ;
	terms:description "Indicates the presence of data which might be considered sensitive" .

:SensitiveDataDescriptionDimension rdf:type dqv:Dimension ;
	terms:title "Description of sensitive data" ;
	terms:description "Provides details about how data might be considered sensitive." .


# Instance data acquisition
:InstanceAcquisitionMeans rdf:type owl:Class ;
	terms:source "Datasheets for datasets, https://arxiv.org/abs/2006.11316"@en ;
	rdfs:comment "Indicates a category to describe how the data associate with instances was acquired"@en ;
	rdfs:label "Instance acquisition means"@en .

# Declare instances instead of subclasses.
:DirectlyObservable rdf:type :InstanceAcquisitionMeans ;
	terms:source "Datasheets for datasets, https://arxiv.org/abs/2006.11316"@en ;
	rdfs:comment "Indicates that a dataset instance was acquired from a directly observable source, e.g. raw text or movie ratings"@en ;
	rdfs:label "Directly observable"@en .

:SubjectReported rdf:type :InstanceAcquisitionMeans ;
	terms:source "Datasheets for datasets, https://arxiv.org/abs/2006.11316"@en ;
	rdfs:comment ""@en ;
	rdfs:label ""@en .

:IndirectInferenceDerived rdf:type :InstanceAcquisitionMeans ;
	terms:source "Datasheets for datasets, https://arxiv.org/abs/2006.11316"@en ;
	rdfs:comment ""@en ;
	rdfs:label ""@en .

