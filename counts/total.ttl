# Distribution date
:DistributionDateDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates the date that the dataset will be distributed"@en ;
	rdfs:label "Distribution date"@en .

:DistributionDateMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the distribution date dimension of a dataset"@en ;
	rdfs:label "Distribution date metric"@en ;
	dqv:inDimension :DistributionDateDimension .


# License
:LicensePresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset will be distributed under a copyright or other intellectual property (IP) license, and/or under applicable terms of use (ToU)"@en ;
	rdfs:label "License"@en .

:LicenseDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of the license and/or ToU, including a link or other access point to, or otherwise reproducing, any relevant licensing terms or ToU, as well as any fees associated with these restrictions"@en ;
	rdfs:label "Description of licesning and/or ToU, including a link or other access point, if applicable"@en .

:LicensePresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the license presence dimension of a dataset"@en ;
	rdfs:label "License presence metric"@en ;
	dqv:inDimension :LicensePresenceDimension .

:LicenseDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the license description dimension of a dataset"@en ;
	rdfs:label "License description metric"@en ;
	dqv:inDimension :LicenseDescriptionDimension .


# Restrictions on instance data
:InstanceDataRestrictionPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether any third parties imposed IP-based or other restrictions on the data associated with the instances"@en ;
	rdfs:label "Third party restrictions on instance data"@en .

:InstanceDataRestrictionDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how third parties impose IP-based or other restrictions on the data associated with the instances, if applicable"@en ;
	rdfs:label "Description of third party restrictions on instance data"@en .

:InstanceDataRestrictionPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the instance data restriction presence dimension of a dataset"@en ;
	rdfs:label "Instance data restriction presence metric"@en ;
	dqv:inDimension :InstanceDataRestrictionPresenceDimension .

:InstanceDataRestrictionDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the instance data restriction description dimension of a dataset"@en ;
	rdfs:label "Instance data restriction description metric"@en ;
	dqv:inDimension :InstanceDataRestrictionDescriptionDimension .


# Export controls / regulatory restrictions
:ExportControlRegulatoryRestrictionPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether any export controls or other regulatory restrictions apply to the dataset or to individual instances"@en ;
	rdfs:label "Export controls / regulatory restrictions"@en .

:ExportControlRegulatoryRestrictionDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes these restrictions, providing a link or other access point to, or otherwise reproducing, any supporting documentation"@en ;
	rdfs:label "Description of export controls / regulatory restrictions, if applicable"@en .

:ExportControlRegulatoryRestrictionPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the export control regulatory restriction presence dimension of a dataset"@en ;
	rdfs:label "Export control regulatory restriction presence metric"@en ;
	dqv:inDimension :ExportControlRegulatoryRestrictionPresenceDimension .

:ExportControlRegulatoryRestrictionDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the export control regulatory restriction description dimension of a dataset"@en ;
	rdfs:label "Export control regulatory restriction description metric"@en ;
	dqv:inDimension :ExportControlRegulatoryRestrictionDescriptionDimension .


####################################################
# Consider maintainer and manager contact as
# Subproperties of the dcat:contactPoint property.
####################################################
# Dataset maintainer
:maintainer rdf:type owl:ObjectProperty ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates who will be supporting / hosting / maintaining the dataset"@en ;
	rdfs:label "Dataset supporter / host / maintainer"@en ;
	rdfs:subPropertyOf dcat:contactPoint .


# Manager contact means
:managerContact rdf:type owl:ObjectProperty ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates how the owner / curator / manager of the dataset can be contacted (e.g., email address)"@en ;
	rdfs:label "Manager contact"@en ;
	rdfs:subPropertyOf dcat:contactPoint .


# Erratum
:ErratumPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset has an erratum"@en ;
	rdfs:label "has erratum"@en .

:ErratumAccessPointDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Provides a link or other access point to the erratum, if applicable"@en ;
	rdfs:label "Erratum access point"@en .

:ErratumPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the erratum presence dimension of a dataset"@en ;
	rdfs:label "Erratum presence metric"@en ;
	dqv:inDimension :ErratumPresenceDimension .

:ErratumAccessPointMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the erratum access point dimension of a dataset"@en ;
	rdfs:label "Erratum access point metric"@en ;
	dqv:inDimension :ErratumAccessPointDimension .


# Update details
:UpdatePresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether the dataset will be updated (e.g., to correct labelling errors, add new instances, delete instances)"@en ;
	rdfs:label "Dataset updates"@en .

:UpdateFrequencyDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how often updates will be communicated to dataset consumers, if applicable"@en ;
	rdfs:label "Dataset updates frequency"@en .

:UpdaterDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates who will be updating the dataset"@en ;
	rdfs:label "Dataset updater"@en .

:UpdateCommunicationDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Describes how updates will be communicated to dataset consumers, if applicable"@en ;
	rdfs:label "Dataset communication means"@en .

:UpdatePresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the update presence dimension of a dataset"@en ;
	rdfs:label "Update presence metric"@en ;
	dqv:inDimension :UpdatePresenceDimension .

:UpdateFrequencyMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the update frequency dimension of a dataset"@en ;
	rdfs:label "Update frequency metric"@en ;
	dqv:inDimension :UpdateFrequencyDimension .

:UpdaterMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the updater dimension of a dataset"@en ;
	rdfs:label "Updater metric"@en ;
	dqv:inDimension :UpdaterDimension .

:UpdateCommunicationMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the update communication dimension of a dataset"@en ;
	rdfs:label "Update communication metric"@en ;
	dqv:inDimension :UpdateCommunicationDimension .


# Data retention limits
:DataRetentionPresenceDimension rdf:type dqv:Dimension ;
    terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
    rdfs:comment "If the dataset relates to people, indicates whether there are applicable limits on the retention of the data associated with the instances (e.g., were the individuals in question told that their data would be retained for a fixed period of time and then deleted)"@en ;
	rdfs:label "Data retention"@en .

:DataRetentionDescriptionDimension rdf:type dqv:Dimension ;
    terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
    rdfs:comment "Describes data retention limits and how they will be enforced, if applicable"@en ;
	rdfs:label "Data retention description"@en .

:DataRetentionPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the data retention presence dimension of a dataset"@en ;
	rdfs:label "Data retention presence metric"@en ;
	dqv:inDimension :DataRetentionPresenceDimension .

:DataRetentionDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the data retention description dimension of a dataset"@en ;
	rdfs:label "Data retention description metric"@en ;
	dqv:inDimension :DataRetentionDescriptionDimension .


# Legacy support
:LegacySupportPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether older versions of the dataset continue to be supported / hosted / maintained"@en ;
	rdfs:label "Legacy support"@en .

# N.B. The following must be filled whether legacy support is enabled or not
:LegacySupportDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of how legacy versions will or will not be supported / hosted / maintained."@en ;
	rdfs:label "Legacy support description"@en .

:LegacySupportPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the legacy support presence dimension of a dataset"@en ;
	rdfs:label "Legacy support presence metric"@en ;
	dqv:inDimension :LegacySupportPresenceDimension .

:LegacySupportDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the legacy support description dimension of a dataset"@en ;
	rdfs:label "Legacy support description metric"@en ;
	dqv:inDimension :LegacySupportDescriptionDimension .


# Augmentation mechanism
:AugmentationPossibilityPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there is a mechanism through which other may extend/augment/build on / contribute to the dataset"@en ;
	rdfs:label "Augmentation possibility"@en .

:AugmentationPossibilityDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of how others may extend/augment/build on / contribute to the dataset, if applicable"@en ;
	rdfs:label "Augmentation possibility description"@en .

:AugmentationValidationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether contributions to the dataset will be validated/verified, if applicable"@en ;
	rdfs:label "Augmentation validation"@en .

# N.B. also needs to be filled out if no validation is carried out
:AugmentationValidationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of how contributions to the dataset will be validated/verified, or why it will not be validated/verified, if applicable"@en ;
	rdfs:label "Augmentation validation description"@en .

:AugmentationCommunicationPresenceDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Indicates whether there is a process for communicating / distributing contributions to dataset consumers"@en ;
	rdfs:label "Augmentation communication"@en .

:AugmentationCommunicationDescriptionDimension rdf:type dqv:Dimension ;
	terms:source "Datasheets for datasets, https://arxiv.org/pdf/1803.09010."@en ;
	rdfs:comment "Description of the process for communicating / distributing contributions to dataset consumers"@en ;
	rdfs:label "Augmentation communication description"@en .

:AugmentationPossibilityPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the augmentation possibility presence dimension of a dataset"@en ;
	rdfs:label "Augmentation possibility presence metric"@en ;
	dqv:inDimension :AugmentationPossibilityPresenceDimension .

:AugmentationPossibilityDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the augmentation possibility description dimension of a dataset"@en ;
	rdfs:label "Augmentation possibility description metric"@en ;
	dqv:inDimension :AugmentationPossibilityDescriptionDimension .

:AugmentationValidationPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the augmentation validation presence dimension of a dataset"@en ;
	rdfs:label "Augmentation validation presence metric"@en ;
	dqv:inDimension :AugmentationValidationPresenceDimension .

:AugmentationValidationDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the augmentation validation description dimension of a dataset"@en ;
	rdfs:label "Augmentation validation description metric"@en ;
	dqv:inDimension :AugmentationValidationDescriptionDimension .

:AugmentationCommunicationPresenceMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the augmentation communication presence dimension of a dataset"@en ;
	rdfs:label "Augmentation communication presence metric"@en ;
	dqv:inDimension :AugmentationCommunicationPresenceDimension .

:AugmentationCommunicationDescriptionMetric rdf:type dqv:Metric ;
	rdfs:comment "A metric to measure the augmentation communication description dimension of a dataset"@en ;
	rdfs:label "Augmentation communication description metric"@en ;
	dqv:inDimension :AugmentationCommunicationDescriptionDimension .

